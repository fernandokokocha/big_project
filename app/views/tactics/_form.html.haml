= form_for @tactic do |f|
  - if @tactic.errors.any?
    #error_explanation
      %h2= "#{pluralize(@tactic.errors.count, "error")} prohibited this tactic from being saved:"
      %ul
        - @tactic.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :gk
    = f.text_field :gk
  .field
    = f.label :d1
    = f.text_field :d1
  .field
    = f.label :d2
    = f.text_field :d2
  .field
    = f.label :d3
    = f.text_field :d3
  .field
    = f.label :d4
    = f.text_field :d4
  .field
    = f.label :dm1
    = f.text_field :dm1
  .field
    = f.label :dm2
    = f.text_field :dm2
  .field
    = f.label :am1
    = f.text_field :am1
  .field
    = f.label :am2
    = f.text_field :am2
  .field
    = f.label :s1
    = f.text_field :s1
  .field
    = f.label :s2
    = f.number_field :s2
  .actions
    = f.submit 'Save'
